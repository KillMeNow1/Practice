#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int power (int a, int b)
{
	int result = pow(a, b);
	printf("The result is %d.", result);
}

int main (int argc, char *argv [])
{
int a;
int b;
if (argc >= 3) 
{
	a = atoi(argv[1]);
	b = atoi(argv[2]);
	printf("a is %d and b is %d.", a, b);
}
else
{
	printf("error");
}	
power (a, b);
printf("\n");
return (0);
}	

